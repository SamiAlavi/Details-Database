#File name: Fake.py
#kivy 1.9.1

#: import WipeTransition kivy.uix.screenmanager.WipeTransition

ScreenManagement:
	transition: WipeTransition()
	Main:
	LoginScreen:
	SignUpScreen:
	Database:
	PersonalDetails:
	CreditCardDetails:
	PassportDetails:
	CarDetails:
	Notes:
	ViewPersonalDetails:
	ViewCreditCardDetails:
	ViewPassportDetails:
	ViewCarDetails:
	ViewNotes:

	
<Main>:
	name:"Main"
	FloatLayout:
		Image:
			source: 'img/bts.zip'
			anim_delay: 1/20
			allow_stretch: False
		Label:
			font_name: 'fonts/DESIB___.ttf'
			text: 'Walletz'
			size_hint: 1,0.1
			pos_hint: {'x':0,'top':0.93}
			font_size:100
			outline_width: 5
			outline_color: 0,0,0
			
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'Login'
			size_hint: 0.3,0.1
			pos_hint: {'x':0.1,'top':0.21}
			font_size: 20
			on_release: app.root.current = "LoginScreen"
			background_normal: 'img/buttonBG.png'
			
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'SignUp'
			size_hint: 0.3,0.1
			pos_hint: {'x':0.6,'top':0.21}
			font_size: 20
			on_release: app.root.current = "SignUpScreen"
			background_normal: 'img/buttonBG.png'
			
<SignUpScreen>:
	name:"SignUpScreen"
	canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "img/bts.jpg"
	FloatLayout:
		Label:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'Please provide a master password that will be used to encrypt your details'
			font_size: 40
			size_hint: 1,0.1
			pos_hint: {'x':0,'top':0.9} 
		Label:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'Username'
			font_size: 20
			size_hint: 0.5,0.1
			pos_hint: {'x':0,'top':0.7}
		TextInput:
			id: username_input
			size_hint: 0.5,0.05
			pos_hint: {'x':0.4,'top':0.675}
			multiline: False
			background_color: 0,0,0,0.7
			write_tab: False
			foreground_color: 1,1,1,1
			
		Label:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'Password'
			font_size: 20
			size_hint: 0.5,0.1
			pos_hint: {'x':0,'top':0.6}
		TextInput:
			id: password_input
			size_hint: 0.5,0.05
			pos_hint: {'x':0.4,'top':0.575}
			password: True
			multiline: False
			background_color: 0,0,0,0.7
			write_tab: False
			foreground_color: 1,1,1,1
			
			
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'			
			text: 'Continue'
			font_size: 20
			size_hint: 0.35,0.1
			pos_hint: {'x':0.1,'y':0.1}
			on_release: root.signup()
			background_normal: 'img/buttonBG.png'
			
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'			
			text: 'Back'
			font_size: 20
			size_hint: 0.35,0.1
			pos_hint: {'x':0.55,'y':0.1}
			on_release: app.root.current = "Main"
			background_normal: 'img/buttonBG.png'

<LoginScreen>:
	name:"LoginScreen"
	canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "img/bts.jpg"
	FloatLayout:
		Label:
			font_name: 'fonts/BLKCHCRY.ttf'
			id: username_label
			text: "Username"
			font_size: 20
			size_hint: 0.2,0.05
			pos_hint: {'x':0,'top':0.7}
			
		TextInput:
			id: username_input			
			size_hint: 0.7,0.05
			pos_hint: {'x':0.2,'top':0.7}
			multiline: False
			background_color: 0,0,0,0.7
			write_tab: False
			foreground_color: 1,1,1,1
		
		Label:
			font_name: 'fonts/BLKCHCRY.ttf'
			id: password_label
			text: 'Password'
			font_size: 20
			size_hint: 0.2,0.05
			pos_hint: {'x':0,'top':0.6}
			
		TextInput:
			id: password_input			
			size_hint: 0.7,0.05
			pos_hint: {'x':0.2,'top':0.6}
			password: True
			multiline: False
			background_color: 0,0,0,0.7
			write_tab: False
			foreground_color: 1,1,1,1
		
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'
			id: login_button
			text: 'Login'
			size_hint: 0.35,0.1
			pos_hint: {'x':0.1,'top':0.2}
			font_size: 20
			on_release: root.login()
			background_normal: 'img/buttonBG.png'
			
		
		Button:
			font_name: 'fonts/BLKCHCRY.ttf'
			text: 'Back'
			size_hint: 0.35,0.1
			pos_hint: {'x':0.55,'top':0.2}
			font_size: 20
			on_release: app.root.current = "Main"	
			background_normal: 'img/buttonBG.png'		
			
